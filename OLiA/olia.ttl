        owl:versionInfo     <http://www.isocat.org/datcat/DC-1416> .

olia:hasOwnerNumber a owl:ObjectProperty ;
        rdfs:comment        "\n\t\t\t\tThe EAGLES-recommended attribute Possessive accounts for the fact that a possessive \n\t\t\t\tpronoun or possessive determiner may have two different numbers. This attribute handles \n\t\t\t\tthe number which is inherent to the possessive form (e.g. Italian \"(la) mia\", \"(la) \n\t\t\t\tnostra\" as first-person singular and first-person plural) as contrasted with the number \n\t\t\t\tit has by virtue of agreeing with a particular noun (e.g. Italian \"(la) mia\", \"(le) mie)\".  \t\t\n\t\t\t\t(http://www.ilc.cnr.it/EAGLES96/annotate/node17.html#recc  20.11.06)\n\t\t\n                Indication concerning the number of the person that ownes something.\n                (http://www.isocat.org/datcat/DC-1417)\n\t\t\t\t\t\t\t\t\n\t\t\t\tInherent plural or singular of possessive pronouns and possessive determiners.\n\t\t" ;
        rdfs:label          "owner number" ;
        rdfs:subPropertyOf  olia_system:hasFeature ;
        owl:versionInfo     "\n\t\tEAGLES feature for the inherent plural/singular of possessive pronouns and possessive determiners;\n\t\thttp://www.isocat.org/datcat/DC-1417, does not entail hasNumber (against hasOwnedNumber), \n\t\tbecause hasNumber applies to agreement information\n\t  " .

olia:hasOwnerPerson a owl:ObjectProperty ;
        rdfs:comment        "\n                            Indication concerning the person that ownes something.\n                                (http://www.isocat.org/datcat/DC-1929)\n                            " ;
        rdfs:label          "owner person" ;
        rdfs:range          olia_top:PersonFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature ;
        owl:versionInfo     <http://www.isocat.org/datcat/DC-1929> , "does not entail hasPerson: hasPerson pertains to agreement" .

olia:hasParent a owl:ObjectProperty ;
        rdfs:comment        "In hierarchical relations, the parent is modelled as source, the child as target."^^xsd:string ;
        rdfs:subPropertyOf  olia_system:hasSource .

olia:hasPerson a owl:ObjectProperty ;
        rdfs:domain         [ a owl:Class ;
                              owl:unionOf  ( olia:Verb olia:PronounOrDeterminer )
                            ] ;
        rdfs:range          olia_top:PersonFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature .

olia:hasPolarity a owl:ObjectProperty ;
        rdfs:range          olia_top:PolarityFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature .

olia:hasProximity a owl:ObjectProperty ;
        rdfs:range          olia_top:ProximityFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature .

olia:hasReduplicationType a owl:ObjectProperty ;
        rdfs:range          olia_top:ReduplicationTypeFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature ;
        owl:versionInfo     "cf. http://www.isocat.org/datcat/DC-2240" .

olia:hasReferentType a owl:ObjectProperty ;
        rdfs:range          olia_top:ReferentTypeFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature ;
        owl:versionInfo     "http://www.isocat.org/datcat/DC-1376 (referentType)" .

olia:hasReflexivity a owl:ObjectProperty ;
        rdfs:domain         olia:Verb ;
        rdfs:range          olia_top:ReflexivityFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature .

olia:hasRegister a owl:ObjectProperty ;
        rdfs:comment        "\n                            Classification indicating the relative level of language individually assigned to a lexeme or term or to a text type.\n                                (ISO12620; http://www.isocat.org/datcat/DC-1988)\n                            " ;
        rdfs:range          olia_top:RegisterFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature ;
        owl:versionInfo     <http://www.isocat.org/datcat/DC-1988> .

olia:hasSemanticRole a owl:ObjectProperty ;
        rdfs:range          olia_top:SemanticRole ;
        rdfs:subPropertyOf  olia_system:hasFeature .

olia:hasSemanticRole a owl:ObjectProperty ;
        rdfs:range          olia_top:SemanticRole ;
        rdfs:subPropertyOf  olia_system:hasFeature .

olia:hasSemanticValency a owl:ObjectProperty ;
        rdfs:comment        "\n\t\tSemantic valency pertains to the number of semantic arguments a predicate takes.\n\t\tThis is not necessarily the same as syntactic valency: In English, expletive \"it\" is\n\t\tused as a syntactic argument if no semantic argument is available, cf. van Valin and \n\t\tLapolla (1997).\n\t" ;
        rdfs:range          olia_top:ValencyFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature ;
        owl:versionInfo     "Ch. Chiarcos" .

olia:hasSentenceConjunct a owl:Class ;
        rdfs:subClassOf  olia:hasConjunct ;
        rdfs:subClassOf  [ a owl:Restriction ;
                           owl:onProperty      olia:hasChild ;
                           owl:someValuesFrom  [ a owl:Class ;
                                                 owl:unionOf  ( olia:Sentence olia:Clause )
                                               ]
                         ] ;
        owl:versionInfo  "http://purl.org/olia/mte/multext-east.owl#SentenceCoordinatingConjunction" .

olia:hasSeparability a owl:ObjectProperty ;
        rdfs:range          olia_top:SeparabilityFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature .

olia:hasSpecificity a owl:ObjectProperty ;
        rdfs:range          olia_top:SpecificityFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature .

olia:hasStrength a owl:ObjectProperty ;
        rdfs:domain         olia:PronounOrDeterminer ;
        rdfs:range          olia_top:StrengthFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature .

olia:hasSubjectNumber a owl:ObjectProperty ;
        rdfs:comment        "\n\t\t\tNumber of the subject of a transitive verb. (By default, this is the meaning of hasNumber).\n\t\t" ;
        rdfs:domain         [ a owl:Class ;
                              owl:intersectionOf  ( olia:Verb _:b14 )
                            ] ;
        rdfs:label          "subject number" ;
        rdfs:range          olia_top:NumberFeature ;
        rdfs:subPropertyOf  olia:hasNumber ;
        owl:versionInfo     "introduced for double agreement of transitive verbs, e.g., in Hungarian, see http://purl.org/olia/mte/multext-east.owl#FirstSgSecondSg" .

olia:hasSubjectPerson a owl:ObjectProperty ;
        rdfs:comment        "\n                            person for the subject of the verb \n\t\t\t\t\t\t\t(in languages with double congruency)\n                                (http://www.isocat.org/datcat/DC-2278)\n                            " ;
        rdfs:domain         olia:Verb ;
        rdfs:label          "subject person" ;
        rdfs:subPropertyOf  olia:hasPerson ;
        owl:versionInfo     "http://www.isocat.org/datcat/DC-2278 (subject person)" .

olia:hasSubordType a owl:ObjectProperty ;
        rdfs:domain         olia:SubordinatingConjunction ;
        rdfs:subPropertyOf  olia_system:hasFeature ;
        owl:deprecated      "remodelled within the hierarchy" .

olia:hasSyntacticFunction a owl:ObjectProperty ;
        rdfs:comment        "\n\t\t\tRelation to be used when the syntactic function of a constituent is different from its morphosyntactic type, \n\t\t\tcf. FormFunctionDiscrepancy in the PTB bracketing guidelines, Bies et al. (1995, §2.2.1)\n\t\t" ;
        rdfs:range          olia_top:SyntacticFunction ;
        rdfs:subPropertyOf  olia_system:hasFeature ;
        owl:versionInfo     "Bies et al. 1995" , "TODO: apply this to primitive concepts, too" .

olia:hasSyntacticRole a owl:ObjectProperty ;
        rdfs:domain         olia_system:UnitOfAnnotation ;
        rdfs:range          olia_top:SyntacticRole ;
        rdfs:subPropertyOf  olia_system:hasFeature .

olia:hasSyntacticValency a owl:ObjectProperty ;
        rdfs:comment        "\n\t\tIn annotation schemes for morphosyntax, \"valency\" normally means syntactic valency, i.e., the number of syntactic arguments \n\t\ta verb/clause takes.\n\t" ;
        rdfs:range          olia_top:ValencyFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature ;
        owl:versionInfo     "SUSANNE (Sampson 1995)" .

olia:hasTense a owl:ObjectProperty ;
        rdfs:domain         olia:Verb ;
        rdfs:range          olia_top:TenseFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature .

olia:hasUsage a owl:ObjectProperty ;
        rdfs:comment        "\n                            The relative commonness with which a term occurs.\n                                (ISO12620; http://www.isocat.org/datcat/DC-1965)\n                            " ;
        rdfs:range          olia_top:UsageAndFrequencyFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature ;
        owl:versionInfo     <http://www.isocat.org/datcat/DC-1965> .

olia:hasValency a owl:ObjectProperty ;
        rdfs:range          olia_top:ValencyFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature .

olia:hasVoice a owl:ObjectProperty ;
        rdfs:range          olia_top:VoiceFeature ;
        rdfs:subPropertyOf  olia_system:hasFeature .

olia:hasWordConjunct a owl:Class ;
        rdfs:subClassOf  olia:hasConjunct ;
        rdfs:subClassOf  [ a owl:Restriction ;
                           owl:onProperty      olia:hasChild ;
                           owl:someValuesFrom  olia:Token
                         ] ;
        owl:versionInfo  "http://purl.org/olia/mte/multext-east.owl#WordCoordinatingConjunction" .

olia:ing a owl:Class ;
        rdfs:comment     "English verb forms ending in '-ing' that represent either Gerunds or Participles." ;
        rdfs:subClassOf  olia:Participle , olia:Gerund ;
        owl:versionInfo  "\n\t\tIntroduced in accordance with EAGLES, where 'Ing' is suggested as a cover term for the Gerund-Participle-Merger in English.\n\t\tThis is, however, a language-specific phenomenon and should instead be represented by multiple inheritance from \n\t\tOLiA Reference Model concepts.\n\t" .

olia:zuInclusion a owl:Class ;
        rdfs:comment     "\n                            Inclusion of zu.\n                                (DFKI; http://www.isocat.org/datcat/DC-1954)\n                            " ;
        rdfs:label       "zu inclusion" ;
        rdfs:subClassOf  olia:Infix ;
        owl:versionInfo  <http://www.isocat.org/datcat/DC-1954> .

