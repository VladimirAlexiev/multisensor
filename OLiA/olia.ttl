        rdfs:subClassOf   olia_top:ModalityFeature .

olia:ObliqueCase a owl:Class ;
        rdfs:comment     "\n\t\tCase that is used when a noun is the object of a verb or a\n\t\tproposition, except for nominative and vocative case. \n\t\t(http://www.isocat.org/datcat/DC-1336)\n\t\t"@en ;
        rdfs:label       "oblique case"@en ;
        rdfs:subClassOf  olia_top:CaseFeature ;
        owl:versionInfo  "http://www.isocat.org/datcat/DC-1336; in EAGLES applied to non-subject pronouns in English and Dutch" .

olia:ObliquePassive a owl:Class ;
        rdfs:comment     "\n\t\t\tA Passive in which a basic Oblique nominal assumes the Subject relation \n\t\t\tin a corresponding nonbasic configuration.  Can include locative passives, \n\t\t\tbenefactive passives and instrumental passives. (Klaiman 1991:23)\n\t\t\t(http://purl.org/linguistics/gold/ObliquePassive)\n\t\t"^^xsd:string ;
        rdfs:subClassOf  olia:PassiveVoice ;
        owl:versionInfo  "http://purl.org/linguistics/gold/ObliquePassive" .

olia:ObliqueRole a owl:Class ;
        rdfs:comment     "\n\t\t\tA semantic role which is not straightforward.\n\t\t\t(http://languagelink.let.uu.nl/tds/onto/LinguisticOntology.owl#obliqueCase)\n\t\t"^^xsd:string ;
        rdfs:subClassOf  olia_top:SemanticRole ;
        owl:versionInfo  "http://languagelink.let.uu.nl/tds/onto/LinguisticOntology.owl#obliqueRole" .

olia:OldUsage a owl:Class ;
        rdfs:comment     "\n                            Used in the past.\n                                (http://www.isocat.org/datcat/DC-1961)\n                            " ;
        rdfs:label       "old" ;
        rdfs:subClassOf  olia:TemporallyDefinedUsage ;
        owl:versionInfo  <http://www.isocat.org/datcat/DC-1961> , "subClassOf dating (dcif:conceptualDomain)" .

olia:OmittedUnit a owl:Class ;
        rdfs:comment     "*U*\nâ ´ unit ...\nThis element marks the interpreted position of a unit symbol, such as $, # (British pounds), FFr (French francs), C$, US$, HK$, A$, M$, S$, and NZ$. It may also appear after % or even cents, when convenient. See section 11 [Modification of NP] for more details on the use of *U*. ...\nIn general, *U* is placed where the word corresponding to the symbol would appear in the string if the text were read aloud. One notable exception is in certain hyphenated compound adjectives, such as a $5-a-share increase (spoken: â ¼A five dollar a share increaseâ ½). Here, the bracketing will usually not reflect the spoken order, with *U* placed as the last element in the ADJP: \n\n  (NP a (ADJP $ 5-a-share *U*)\n      increase)\n\nSometimes, this type may lack the *U* entirely. \n(Bies et al. 1995)"^^xsd:string ;
        rdfs:subClassOf  olia_top:NullElement ;
        owl:versionInfo  "added in conformance with PTB bracketing guidelines (Santorini 1991, Bies et al. 1995)" .

olia:OnceNoun a owl:Class ;
        rdfs:comment      "noun conveying the meaning that the action is performed once" ;
        rdfs:isDefinedBy  <http://www.isocat.org/datcat/DC-3853> ;
        rdfs:subClassOf   olia:CommonNoun ;
        owl:deprecated    "true" ;
        owl:versionInfo   "onceNoun [DC-3853] ``noun conveying the meaning that the action is performed once''. Clearly, this noun carries a meaning that is closely related to aspect information (e.g., SemelfactiveAspect), but as aspect is a verb-centered concept, it would have been counter-intuitive to model this as Noun and hasAspect some SemelfactiveAspect. Hence, this concept is directly adopted in the OLiA Reference Model, but marked as deprecated until an ontologically satisfying modeling of aspect-related features of nouns has been achieved." .

olia:OpenAngleBracket a owl:Class ;
        rdfs:comment     "<\n*LAB*\nLeft angle bracket\n(Santorini 1991)"^^xsd:string ;
        rdfs:subClassOf  olia:LeftParentheticalPunctuation ;
        owl:versionInfo  "PTB bracketing guidelines, Santorini 1991" .

